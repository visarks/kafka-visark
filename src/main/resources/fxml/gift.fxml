<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.podigua.kafka.gift.controller.GiftController"
            prefHeight="380.0" prefWidth="679.0">
<VBox alignment="TOP_CENTER" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
    <HBox>
        <VBox spacing="5">
            <HBox HBox.hgrow="ALWAYS" alignment="CENTER">
                <ImageView fx:id="wechat" fitHeight="339" fitWidth="339"/>
            </HBox>
            <HBox alignment="CENTER" spacing="10">
                <FontIcon fx:id="wecharIcon"/>
                <Label text="微信"/>
            </HBox>
        </VBox>
        <VBox spacing="5">
            <HBox HBox.hgrow="ALWAYS">
                <ImageView fx:id="alipay" fitHeight="339" fitWidth="339"/>
            </HBox>
            <HBox alignment="CENTER"  spacing="10">
                <FontIcon fx:id="alipayIcon"/>
                <Label text="支付宝"/>
            </HBox>
        </VBox>
    </HBox>
</VBox>
</AnchorPane>
